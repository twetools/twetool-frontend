{
  "name": "twetool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db-update": "cd ../twetool-backend && dotnet ef database update",
    "start-backend": "npm run sync-backend-env && cd ../twetool-backend && dotnet watch run",
    "start-frontend": "cd ../twetool-frontend && next dev",
    "sync-backend-env": "node ../twetool-backend/scripts/sync-environment.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "backup-project": "scripts\\backup\\backup-project.bat",
    "clean-vscode": "start scripts\\maintenance\\cleanup-cache.bat",
    "check-ts-errors": "npx tsc --noEmit",
    "git-reset-menu": "git log --oneline",
    "remove-empty-files": "node scripts/maintenance/removeEmptyFiles.js",
    "upgrade-tools": "node scripts/maintenance/upgrade-tools.js"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@heroicons/react": "^2.2.0",
    "@popperjs/core": "^2.11.8",
    "@prisma/client": "^6.13.0",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19",
    "apexcharts": "^4.3.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "flatpickr": "^4.6.13",
    "mssql": "^11.0.1",
    "mysql2": "^3.14.1",
    "next": "^15.5.0",
    "postcss": "^8",
    "prisma": "^6.13.0",
    "react": "^19.1.1",
    "react-apexcharts": "^1.7.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "reflect-metadata": "^0.2.2",
    "simplebar-react": "^3.3.0",
    "swiper": "^11.2.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.11",
    "tree-cli": "^0.6.7",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "@react-jvectormap/core": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    },
    "@react-jvectormap/world": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    }
  }
}
