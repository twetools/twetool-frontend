{
  "files.watcherExclude": {
    "**/scripts/testing/**": true
  },
  "terminal.integrated.profiles.windows": {
    "PowerShell - Default": {
      "source": "PowerShell",
      "args": ["-NoLogo", "-ExecutionPolicy", "Bypass"]
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell - Default",
  "terminal.integrated.tabs.enabled": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/.next": true,
    "**/out": true,
    "**/dist": true,
    "**/build": true,
    "**/.turbo": true,
    "**/.vercel": true,
    "**/.cache": true,
    "**/coverage": true
  },
  "chat.instructionsFilesLocations": {
    ".github/instructions": true
  }
}